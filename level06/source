            puts("\x1b[32m.---------------------------.");
            puts("\x1b[31m| !! TAMPERING DETECTED !!  |");
            puts("\x1b[32m\'---------------------------\'");
            ret = 1;
        }
        else {
            dupSerial = ((int)login[3] ^ 4919) + 6221293;
            for (i = 0; i < (int)size; i = i + 1) {
                if (login[i] < ' ') {
                    return 1;
                }
